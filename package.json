{
  "name": "@fullcalendar/vue",
  "version": "5.0.0-beta.4",
  "title": "FullCalendar Vue Component",
  "description": "An official FullCalendar component for Vue",
  "keywords": [
    "vue",
    "calendar",
    "fullcalendar"
  ],
  "docs": "https://fullcalendar.io/docs/vue",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcalendar/fullcalendar-vue.git"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf dist tmp",
    "webpack": "webpack",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "test:ci": "npm run test:src:build && npm run test:karma:ci",
    "test": "concurrently npm:test:src:watch npm:test:karma:watch",
    "test:src:build": "rollup -c --environment BUILD:production",
    "test:src:watch": "rollup -c --environment BUILD:development --watch",
    "test:karma:ci": "karma start --browsers ChromeHeadless --single-run --no-auto-watch",
    "test:karma:watch": "karma start",
    "lint": "eslint src tests *.js",
    "ci": "./scripts/ci.sh"
  },
  "dependencies": {
    "@fullcalendar/core": "5.0.0-beta.4"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.8.4",
    "@fullcalendar/daygrid": "5.0.0-beta.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "module": "dist/main.js",
  "files": [
    "dist"
  ]
}
