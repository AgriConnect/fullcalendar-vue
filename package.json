{
  "name": "fullcalendar-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist demo/built",
    "watch": "rollup -c --watch",
    "build": "rollup -c",
    "build:umd": "rollup -c --config rollup.config.component.js",
    "build:esm": "rollup -c --config rollup.config.component.js --format es --file dist/main.esm.js",
    "dist": "npm run build:umd && npm run build:esm && npm run dist:meta",
    "dist:meta": "cp README.* LICENSE.* dist && ./scripts/generate-package-config",
    "test": "jest",
    "lint": "eslint src demo *.js",
    "preversion": "npm run clean",
    "version": "npm run lint && npm run dist && npm run test && git add -f dist package-lock.json"
  },
  "dependencies": {
    "@fullcalendar/core": "^4.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@fullcalendar/daygrid": "^4.0.1",
    "@fullcalendar/interaction": "^4.0.1",
    "@fullcalendar/timegrid": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "buble": "^0.19.7",
    "buble-jest": "^1.0.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.5.0",
    "postcss-import": "^12.0.1",
    "rollup": "^1.7.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.6.10"
  }
}
