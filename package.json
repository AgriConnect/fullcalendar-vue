{
  "name": "arshaw-fullcalendar-vue",
  "version": "0.1.1",
  "title": "FullCalendar Vue Component",
  "description": "An official FullCalendar component for Vue",
  "docs": "https://fullcalendar.io/docs/vue",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "watch": "rollup -c --watch",
    "build": "rollup -c",
    "dist": "npm run build && npm run dist:meta",
    "dist:meta": "cp README.* LICENSE.* dist && ./scripts/generate-package-config",
    "test": "jest",
    "lint": "eslint src *.js",
    "preversion": "npm run clean",
    "version": "npm run lint && npm run dist && npm run test && git add -f dist package-lock.json",
    "ci": "npm run dist && npm run test && npm run lint"
  },
  "dependencies": {
    "@fullcalendar/core": "^4.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@fullcalendar/daygrid": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "buble": "^0.19.7",
    "buble-jest": "^1.0.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.5.0",
    "rollup": "^1.7.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-node-resolve": "^4.0.1",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.6.10"
  }
}
